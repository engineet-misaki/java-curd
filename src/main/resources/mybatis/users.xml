<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.store_sample.store.infrastructure.users.UserMapper">
  <insert id="insert" parameterType="com.store_sample.store.domain.users.model.SigninUser">
    INSERT INTO users
    (id, name, password, role)
    VALUES
    (#{id}, #{name}, #{password}, #{role})
  </insert>
</mapper>